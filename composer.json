{
    "name": "theduman/laravel-docsify",
    "description": "Add Docsify to Laravel",
    "version": "1.0",
    "authors": [
        {
            "name": "Emre Duman",
            "email": "emreduman16@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../packages/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Theduman\\Docsify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Theduman\\Docsify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Theduman\\Docsify\\DocsifyServiceProvider"
            ]
        }
    }
}
